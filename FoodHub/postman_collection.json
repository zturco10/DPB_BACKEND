					"name": "LoginUsuario",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the response JSON\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"// Check if the token exists in the response\r",
									"// (Adjust jsonData to jsonData.token if your API returns {\"token\": \"...\"})\r",
									"if (jsonData) {\r",
									"    // Store the token in the collection variable 'authToken'\r",
									"    pm.collectionVariables.set(\"authToken\", jsonData);\r",
									"    console.log(\"Auth Token set from login response.\");\r",
									"} else {\r",
									"    console.log(\"Token not found in login response.\");\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"correo\": \"estudiante@ejemplo.com\",\r\n  \"contrasena\": \"contraseñaSegura123\"\r\n}",
